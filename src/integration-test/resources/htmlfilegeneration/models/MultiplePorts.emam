package models;

component MultiplePorts {
  ports
    in Q(0 m : 0.1 m : 10 km)^{2,3} m1,
    in Q(5 Hz : 20 kHz)^{3,1} m2,
    in Q(-2 mm/h : 2 km/s) c,
    out Q(-oo km/h : oo km/h)^{2,1} v,
    out Q(-oo km^-1 : oo m^-1) k;

  implementation Math {
    v = (m1*m2) + c;
    Q temp = 0;
    for i=1:3
      temp = temp + m2(i,1)*c;
    end
    k = temp;
  }
}